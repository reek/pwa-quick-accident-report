{
  "name": "pwa-quick-accident-report",
  "description": "This application is intended to help the driver during a traffic accident.",
  "version": "1.0.0",
  "author": "Ricardo Reves",
  "homepage": "https://cadze.ch/",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "dev:gh": "ng serve --deploy-url /pwa-qar-front/ --base-href /pwa-qar-front/",
    "build": "ng build",
    "build:prod": "ng build --prod && npx copy web",
    "build:prod:gh": "ng build --prod --deploy-url /pwa-quick-accident-report/ --base-href /pwa-quick-accident-report/ && cp www/index.html www/404.html && npx copy web",
    "build:prod:ftp": "ng build --prod --deploy-url /pwa/qar/ --base-href /pwa/qar/ && cp .htaccess www/.htaccess && npx copy web",
    "prod": "npm run build:prod && http-server -p 4200 -c-1 www -o",
    "deploy:gh": "gh-pages -d www",
    "deploy:ftp": "node ftp-deploy.js",
    "backend": "cd .. && cd pwa-qar-back && npm run dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": ">=11.0.0",
    "npm": ">=6.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "^7.2.2",
    "@angular/service-worker": "^7.2.2",
    "@capacitor/core": "^1.0.0-beta.19",
    "@ionic-native/core": "^5.0.0",
    "@ionic/angular": "^4.1.0",
    "core-js": "^2.5.4",
    "moment": "^2.24.0",
    "ngx-auto-unsubscribe": "^3.0.0",
    "ol": "^5.3.2",
    "qrcode": "^1.3.4",
    "rxjs": "~6.3.3",
    "tesseract.js": "^2.0.0-alpha.10",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "~7.3.1",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@capacitor/cli": "^1.0.0-beta.19",
    "@ionic/angular-toolkit": "~1.4.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.14.2",
    "@types/ol": "^4.6.5",
    "codelyzer": "~4.5.0",
    "ftp-deploy": "^2.3.3",
    "gh-pages": "^2.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.1.6"
  }
}
